@{
    Layout = "../Shared/_layout.cshtml";
}

<div id="editor" class="panel">
    <div class="boxhead">
        <h2>Uninstalled</h2>
    </div>
    <div class="boxbody">   
        <div class="padded-content scrolling-content">
            <h2>Package uninstalled</h2>
            <div class="property-pane">
                All items in the package have been uninstalled.            
            </div>
            <h2>What now?</h2>
            <div class="property-pane">
                &bull; <a href="@Url.Action("LocalRepository")">Return to local packages</a>
            </div>
        </div>
    </div>
    <div class="boxfooter">
        <div class="statusBar">
            <h5></h5>
        </div>
    </div>
</div>